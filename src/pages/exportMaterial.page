<apex:page controller="exportMaterialController" contentType="application/vnd.ms-excel">
	<apex:pageBlock title="Material">
	        <apex:pageBlockTable value="{!materials}" var="item">
	            <apex:column value="{!item.Name}"/>
	            <apex:column value="{!item.Name__c}"/>
	            <apex:column value="{!item.Quality__c}"/>
	            <apex:column value="{!item.Amount__c}"/>
	            <apex:column value="{!item.Made_in__c}"/>
	            <apex:column value="{!item.Enter_date__c}"/> 
	        </apex:pageBlockTable>
	    </apex:pageBlock>
</apex:page>